<%= simple_form_for @lecture do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :teachers %>
  <%= f.input :location %>
  <%= f.input :max_leads %>
  <%= f.input :max_follows %>
  <%= f.input :start_date %>
  <%= f.input :duration %>
  <%= f.input :status %>
  <%= f.submit :Create, class: 'black-button margin-center' %>
<% end %>
